doctype html
html(lang="ja")
  head
    meta(charset="UTF-8")
    meta(name="viewport", content="width=device-width, initial-scale=1.0")
    meta(http-equiv="X-UA-Compatible", content="ie=edge")
    link(rel="stylesheet", href="https://cdn.jsdelivr.net/npm/uikit@3.9.2/dist/css/uikit.min.css")
    script(src="https://cdn.jsdelivr.net/npm/uikit@3.9.2/dist/js/uikit.min.js")
    script(src="https://cdn.jsdelivr.net/npm/uikit@3.9.2/dist/js/uikit-icons.min.js")
    title スライド一覧
  body
    div(class="uk-background-default uk-padding")
      p(class="uk-h1 uk-text-lead")
        | スライド一覧
      ul(class="uk-list uk-list-circle uk-list-muted uk-link-muted")
        each val in slides
          li
            a(href=val)
              | #{val}
